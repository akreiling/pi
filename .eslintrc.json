{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": 0,
        "camelcase": 0,
        "no-multi-spaces": ["error", { "ignoreEOLComments": true }],
        "no-cond-assign": ["error", "except-parens"],
        "no-return-assign": ["error", "except-parens"]
    }
}
